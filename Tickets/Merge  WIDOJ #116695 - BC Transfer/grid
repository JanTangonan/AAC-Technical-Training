SELECT 
    "KEY", 
    "CUSTODY_OF", 
    "LOCATION", 
    "LAB_ITEM_NUMBER", 
    "DESCRIPTION", 
    "PARENT_ECN", 
    "LAB_CASE", 
    "DEPARTMENT_CASE_NUMBER", 
    "DEPARTMENT_ITEM_NUMBER", 
    "CONTAINER_KEY", 
    "ITEM_TYPE", 
    "CASE_KEY", 
    "CUSTODYLOCATION", 
    "HOLD_IN_DEPARTMENT", 
    "CONTAINER_TYPE", 
    "IN_CONTAINER", 
    "SORT_ORDER",
    '' AS REFUSAL_REASON  
FROM CV_ITEMS_IN_CONTAINER 
ORDER BY DISPLAY_ORDER

