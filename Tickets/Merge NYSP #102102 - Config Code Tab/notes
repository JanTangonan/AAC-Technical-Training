select 
    OFFENSE_CODE, 
    OFFENSE_DESCRIPTION, 
    STATUTE_OF_LIMITATIONS_Y, 
    STATUTE_OF_LIMITATIONS_M, 
    STATUTE_OF_LIMITATIONS_D, 
    NO_STATUTE, 
    TYPICAL_TURN_AROUND_TIME, 
    INCLUDE_IN_PRELOG, 
    QUALIFYING_OFFENSE, 
    ACTIVE 
from TV_OFFENSE  
where (OFFENSE_CODE = '' OR OFFENSE_CODE IS NULL)